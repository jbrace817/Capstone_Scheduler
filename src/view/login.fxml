<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="295.0" prefWidth="444.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label fx:id="loginLbl" alignment="CENTER" layoutX="126.0" layoutY="37.0" prefHeight="30.0" prefWidth="181.0" text="loginLbl">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="userNameLbl" alignment="CENTER" layoutX="84.0" layoutY="101.0" prefHeight="17.0" prefWidth="100.0" text="userNameLbl">
